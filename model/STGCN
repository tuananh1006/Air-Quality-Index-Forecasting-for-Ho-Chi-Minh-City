umerate(stations_coords)}\n","\n","def get_adjacency_matrix(coords, normalized_k=0.0): # 0.0 để giữ mọi kết nối\n","    dist_matrix = squareform(pdist(coords, metric='euclidean'))\n","    std = dist_matrix.std()\n","    adj_mx = np.exp(-np.square(dist_matrix / std))\n","    adj_mx[adj_mx < normalized_k] = 0\n","    return adj_mx\n","\n","adj_mx = get_adjacency_matrix(stations_coords, normalized_k=0.1)\n","print(f\"--> Số lượng trạm (Nodes): {num_nodes}\")\n","\n","# ==========================================\n","# 2. FEATURE ENGINEERING (CYCLIC) & PREPROCESSING\n","# ==========================================\n","df_stgcn = df.copy()\n","# --- MÃ HÓA CHU KỲ (SIN/COS) - QUAN TRỌNG ---\n","# Giúp model hiểu 23h gần 0h, tháng 12 gần tháng 1\n","day_seconds = 24 * 60 * 60\n","year_seconds = 365.2425 * 24 * 60 * 60\n","\n","df_stgcn['Local Time'] = pd.to_datetime(df_stgcn['Local Time'])\n","timestamp_s = df_stgcn['Local Time'].map(pd.Timestamp.timestamp)\n","\n","df_stgcn['day_sin'] = np.sin(timestamp_s * (2 * np.pi / day_seconds))\n","df_stgcn['day_cos'] = np.cos(timestamp_s * (2 * np.pi / day_seconds))\n","df_stgcn['year_sin'] = np.sin(timestamp_s * (2 * np.pi / year_seconds))\n","df_stgcn['year_cos'] = np.cos(timestamp_s * (2 * np.pi / year_seconds))\n","\n","# Định nghĩa target_col trước khi sử dụng\n","target_col = 'Aqi'\n","\n","# Loại bỏ các cột raw integer gây nhiễu\n","exclude_cols = ['Local Time', 'node_id', target_col, 'Month', 'Day', 'Hour', 'Weekday',\n","                'Unnamed: 0', 'Train Name', 'hour', 'month', 'day', 'weekday']\n","feature_cols = [c for c in df_stgcn.columns if c not in exclude_cols]\n","print(f\"--> Features được sử dụng (Đã tối ưu Cyclic): {feature_cols}\")\n","\n","# --- MÃ HÓA CHU KỲ (SIN/COS) ---\n","# Giúp model hiểu 23h và 0h là liền nhau\n","df_stgcn['hour_sin'] = np.sin(2 * np.pi * df_stgcn['Local Time'].dt.hour / 24)\n","df_stgcn['hour_cos'] = np.cos(2 * np.pi * df_stgcn['Local Time'].dt.hour / 24)\n","df_stgcn['month_sin'] = np.sin(2 * np.pi * df_stgcn['Local Time'].dt.month / 12)\n","df_stgcn['month_cos'] = np.cos(2 * np.pi * df_stgcn['Local Time'].dt.month / 12)\n","df_stgcn['day_sin'] = np.sin(2 * np.pi * df_stgcn['Local Time'].dt.weekday / 7)\n","df_stgcn['day_cos'] = np.cos(2 * np.pi * df_stgcn['Local Time'].dt.weekday / 7)\n","\n","# Tạo cột node_id trong df_stgcn\n","df_stgcn['node_id'] = df_stgcn.apply(lambda row: station_to_node_id.get((row['Latitude'], row['Longitude'])), axis=1)\n","\n","# Chọn Features\n","exclude_cols = ['Local Time', 'node_id', target_col, 'Month', 'Day', 'Hour', 'Weekday', 'Unnamed: 0', 'Train Name']\n","feature_cols = [c for c in df_stgcn.columns if c not in exclude_cols]\n","print(f\"--> Features được sử dụng: {feature_cols}\")\n","\n","# --- TẠO GRID & FILL DATA ---\n","all_times = df_stgcn['Local Time'].unique()\n","all_node_ids = np.arange(num_nodes)\n","full_grid = pd.MultiIndex.from_product([all_times, all_node_ids], names=['Local Time', 'node_id']).to_frame(index=False)\n","\n","df_filled = pd.merge(full_grid, df_stgcn, on=['Local Time', 'node_id'], how='left')\n","df_filled = df_filled.set_index(['Local Time', 'node_id']).sort_index()\n","\n","# Impute missing values\n","for col in [target_col] + feature_cols:\n","    df_filled[col] = df_filled.groupby(level='node_id')[col].ffill().bfill()\n","df_filled = df_filled.fillna(0)\n","\n","# ==========================================\n","# 3. CHIA TẬP DATA & SCALING\n","# ==========================================\n","# Reshape to 3D: (Time, Nodes, Features)\n","unique_timestamps = df_filled.index.get_level_values(0).unique().sort_values()\n","data_list = []\n","for t in unique_timestamps:\n","    df_t = df_filled.loc[t].sort_index(level='node_id')\n","    # Target lên đầu tiên\n","    vals = df_t[[target_col] + feature_cols].values\n","    data_list.append(vals)\n","\n","data_3d = np.array(data_list)\n","timestamps = pd.to_datetime(unique_timestamps)\n","\n","# Split by Date\n","train_mask = (timestamps >= '2023-01-01') & (timestamps <= '2024-12-31 23:59:59')\n","val_mask   = (timestamps >= '2025-01-01') & (timestamps <= '2025-06-30 23:59:59')\n","test_mask  = (timestamps >= '2025-07-01') & (timestamps <= '2025-09-30 23:59:59')\n","\n","train_data_raw = data_3d[train_mask]\n","val_data_raw   = data_3d[val_mask]\n","test_data_raw  = data_3d[test_mask]\n","\n","# Scaling\n","scaler = StandardScaler()\n","train_flat = train_data_raw.reshape(-1, train_data_raw.shape[-1])\n","scaler.fit(train_flat)\n","\n","train_data = scaler.transform(train_flat).reshape(train_data_raw.shape)\n","val_data   = scaler.transform(val_data_raw.reshape(-1, val_data_raw.shape[-1])).reshape(val_data_raw.shape)\n","test_data  = scaler.transform(test_data_raw.reshape(-1, test_data_raw.shape[-1])).reshape(test_data_raw.shape)\n","\n","print(f\"--> Train samples: {len(train_data)} | Val: {len(val_data)} | Test: {len(test_data)}\")\n","\n","# ==========================================\n","# 4. SLIDING WINDOW (24h -> 1h)\n","# ==========================================\n","def generate_dataset(data, window_size=24, horizon=1):\n","    length = len(data)\n","    end_index = length - horizon - window_size + 1\n","    X, Y = [], []\n","    index = 0\n","    while index < end_index:\n","        x_window = data[index : index + window_size]\n","        y_window = data[index + window_size : index + window_size + horizon, :, 0] # Chỉ lấy AQI target\n","        X.append(x_window)\n","        Y.append(y_window)\n","        index += 1\n","    return np.array(X), np.array(Y)\n","\n","x_train, y_train = generate_dataset(train_data)\n","x_val, y_val     = generate_dataset(val_data)\n","x_test, y_test   = generate_dataset(test_data)\n","\n","# Transpose Input: (Batch, Features, Time, Nodes)\n","x_train = x_train.transpose(0, 3, 1, 2)\n","x_val   = x_val.transpose(0, 3, 1, 2)\n","x_test  = x_test.transpose(0, 3, 1, 2)\n","\n","# DataLoader\n","batch_size = 32\n","train_loader = DataLoader(TensorDataset(torch.FloatTensor(x_train).to(device), torch.FloatTensor(y_train).to(device)), batch_size=batch_size, shuffle=True)\n","val_loader   = DataLoader(TensorDataset(torch.FloatTensor(x_val).to(device), torch.FloatTensor(y_val).to(device)), batch_size=batch_size, shuffle=False)\n","test_loader  = DataLoader(TensorDataset(torch.FloatTensor(x_test).to(device), torch.FloatTensor(y_test).to(device)), batch_size=batch_size, shuffle=False)\n","\n","num_features = x_train.shape[1]\n","print(\"--> Đã chuẩn bị xong DataLoader.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"HSbpvYJi4ddc","executionInfo":{"status":"ok","timestamp":1764325983341,"user_tz":-420,"elapsed":59790,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}},"outputId":"6a2769b5-fe2c-441d-90ab-594896c256a5"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["--> Số lượng trạm (Nodes): 40\n","--> Features được sử dụng (Đã tối ưu Cyclic): ['Co', 'No2', 'O3', 'Pm10', 'Pm25', 'So2', 'Clouds', 'Precipitation', 'Pressure', 'Relative Humidity', 'Temperature', 'UV_Index', 'Wind Speed', 'Latitude', 'Longitude', 'day_sin', 'day_cos', 'year_sin', 'year_cos']\n","--> Features được sử dụng: ['Co', 'No2', 'O3', 'Pm10', 'Pm25', 'So2', 'Clouds', 'Precipitation', 'Pressure', 'Relative Humidity', 'Temperature', 'UV_Index', 'Wind Speed', 'Latitude', 'Longitude', 'day_sin', 'day_cos', 'year_sin', 'year_cos', 'hour_sin', 'hour_cos', 'month_sin', 'month_cos']\n","--> Train samples: 17160 | Val: 4248 | Test: 2160\n","--> Đã chuẩn bị xong DataLoader.\n"]}]},{"cell_type":"code","source":["# ==========================================\n","# ĐỊNH NGHĨA MODEL STGCN V2\n","# ==========================================\n","\n","class GraphConv(nn.Module):\n","    def __init__(self, c_in, c_out, gso, dropout_rate=0.5):\n","        super(GraphConv, self).__init__()\n","        self.gso = gso\n","        self.theta = nn.Parameter(torch.FloatTensor(c_in, c_out))\n","        self.bias = nn.Parameter(torch.FloatTensor(c_out))\n","        self.dropout = nn.Dropout(dropout_rate)\n","        self.reset_parameters()\n","\n","    def reset_parameters(self):\n","        nn.init.xavier_uniform_(self.theta)\n","        nn.init.zeros_(self.bias)\n","\n","    def forward(self, x):\n","        batch_size, c_in, T, num_nodes = x.shape\n","        x_tmp = x.permute(0, 2, 3, 1)\n","        x_mul = torch.matmul(x_tmp, self.theta)\n","        x_gconv = torch.einsum('btnc,nm->btmc', x_mul, self.gso)\n","        x_gconv = x_gconv + self.bias\n","        return self.dropout(x_gconv.permute(0, 3, 1, 2))\n","\n","class STGCN_Block(nn.Module):\n","    def __init__(self, c_in, c_out, gso, kt=3):\n","        super(STGCN_Block, self).__init__()\n","        self.conv_t1 = nn.Conv2d(c_in, c_out * 2, kernel_size=(1, kt), padding=(0, 1))\n","        self.conv_s = GraphConv(c_out, c_out, gso)\n","        self.conv_t2 = nn.Conv2d(c_out, c_out * 2, kernel_size=(1, kt), padding=(0, 1))\n","        self.residual = nn.Conv2d(c_in, c_out, kernel_size=1) if c_in != c_out else None\n","        self.ln = nn.LayerNorm([c_out, num_nodes])\n","\n","    def forward(self, x):\n","        res = x\n","        if self.residual: res = self.residual(x)\n","        x = self.conv_t1(x)\n","        p, q = x[:, :x.shape[1]//2, :, :], x[:, x.shape[1]//2:, :, :]\n","        x = p * torch.sigmoid(q)\n","        x = self.conv_s(x)\n","        x = self.conv_t2(x)\n","        p, q = x[:, :x.shape[1]//2, :, :], x[:, x.shape[1]//2:, :, :]\n","        x = p * torch.sigmoid(q)\n","        return x + res\n","\n","class STGCN_Model_V2(nn.Module):\n","    def __init__(self, num_nodes, num_features, gso, hidden_dim=128): # Tăng lên 128\n","            super(STGCN_Model_V2, self).__init__()\n","            # Quay về 2 Block nhưng to hơn (Wide instead of Deep)\n","            self.block1 = STGCN_Block(num_features, hidden_dim, gso)\n","            self.block2 = STGCN_Block(hidden_dim, hidden_dim, gso)\n","\n","            self.final_conv = nn.Conv2d(hidden_dim, hidden_dim, kernel_size=1)\n","            self.output_layer = nn.Linear(hidden_dim, 1)\n","\n","    def forward(self, x):\n","        x = self.block1(x)\n","        x = self.block2(x)\n","        x = self.final_conv(x)\n","        x = x[:, :, -1, :] # Lấy bước thời gian cuối\n","        x = x.permute(0, 2, 1)\n","        x = self.output_layer(x)\n","        return x.permute(0, 2, 1)\n","\n","# Chuẩn bị GSO (Normalized Laplacian)\n","def get_normalized_adj(adj):\n","    adj = adj + np.eye(adj.shape[0])\n","    d = adj.sum(1)\n","    d_inv_sqrt = np.power(d, -0.5).flatten()\n","    d_inv_sqrt[np.isinf(d_inv_sqrt)] = 0.\n","    d_mat_inv_sqrt = np.diag(d_inv_sqrt)\n","    return torch.tensor(d_mat_inv_sqrt.dot(adj).dot(d_mat_inv_sqrt), dtype=torch.float32).to(device)\n","\n","gso = get_normalized_adj(adj_mx)\n","\n","# Khởi tạo Model\n","model = STGCN_Model_V2(num_nodes=num_nodes, num_features=num_features, gso=gso, hidden_dim=64).to(device)\n","print(\"--> Đã khởi tạo Model STGCN V2 thành công.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"obfge_b74mcz","executionInfo":{"status":"ok","timestamp":1764325983447,"user_tz":-420,"elapsed":91,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}},"outputId":"704b2591-a01b-4328-d0b1-c0673f9144c9"},"execution_count":5,"outputs":[{"output_type":"stream","name":"stdout","text":["--> Đã khởi tạo Model STGCN V2 thành công.\n"]}]},{"cell_type":"code","source":["import time\n","\n","# --- CẤU HÌNH TRAINING ---\n","criterion = nn.L1Loss()\n","optimizer = torch.optim.Adam(model.parameters(), lr=0.003)\n","\n","# >>> NÂNG CẤP: Dùng Scheduler thông minh theo dõi Loss <<<\n","# factor=0.5: Giảm LR một nửa khi bị kẹt\n","# patience=5: Chờ 5 epoch mà loss không giảm mới bắt đầu hạ LR\n","scheduler = torch.optim.lr_scheduler.ReduceLROnPlateau(optimizer, mode='min', factor=0.5, patience=5)\n","\n","epochs = 100\n","best_val_loss = float('inf')\n","save_path = 'best_stgcn_final.pth'\n","loss_history = {'train': [], 'val': []}\n","\n","# --- CẤU HÌNH EARLY STOPPING ---\n","early_stopping_patience = 15 # Kiên nhẫn 15 epoch trước khi dừng hẳn\n","early_stopping_counter = 0\n","\n","print(f\"=== BẮT ĐẦU TRAINING (SMART SCHEDULER + EARLY STOPPING) ===\")\n","start_time = time.time()\n","\n","for epoch in range(epochs):\n","    # 1. Train\n","    model.train()\n","    train_loss = 0\n","    for batch_x, batch_y in train_loader:\n","        optimizer.zero_grad()\n","        output = model(batch_x)\n","        loss = criterion(output, batch_y)\n","        loss.backward()\n","        torch.nn.utils.clip_grad_norm_(model.parameters(), max_norm=5.0)\n","        optimizer.step()\n","        train_loss += loss.item()\n","\n","    # 2. Validate\n","    model.eval()\n","    val_loss = 0\n","    with torch.no_grad():\n","        for batch_x, batch_y in val_loader:\n","            output = model(batch_x)\n","            val_loss += criterion(output, batch_y).item()\n","\n","    avg_train_loss = train_loss / len(train_loader)\n","    avg_val_loss = val_loss / len(val_loader)\n","    loss_history['train'].append(avg_train_loss)\n","    loss_history['val'].append(avg_val_loss)\n","\n","    # >>> CẬP NHẬT SCHEDULER <<<\n","    # Bước này quan trọng: Truyền val_loss vào để nó tự tính toán\n","    old_lr = optimizer.param_groups[0]['lr']\n","    scheduler.step(avg_val_loss)\n","    new_lr = optimizer.param_groups[0]['lr']\n","\n","    # In thông tin\n","    print(f\"Epoch {epoch+1}/{epochs} | LR: {new_lr:.5f} | Train: {avg_train_loss:.4f} | Val: {avg_val_loss:.4f}\", end='')\n","\n","    # Logic Early Stopping\n","    if avg_val_loss < best_val_loss:\n","        best_val_loss = avg_val_loss\n","        torch.save(model.state_dict(), save_path)\n","        early_stopping_counter = 0\n","        print(f\"  [Save Best] {best_val_loss:.4f}\")\n","    else:\n","        early_stopping_counter += 1\n","        print(f\"  [Wait] {early_stopping_counter}/{early_stopping_patience}\")\n","\n","        if early_stopping_counter >= early_stopping_patience:\n","            print(f\"\\n--> DỪNG SỚM tại Epoch {epoch+1} vì model không còn học được nữa.\")\n","            break\n","\n","print(f\"\\n--> Hoàn tất sau {(time.time() - start_time)/60:.2f} phút.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"vEm32zvk4oaG","executionInfo":{"status":"ok","timestamp":1764326302514,"user_tz":-420,"elapsed":317497,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}},"outputId":"2081299c-6c44-43db-9db6-f6017cb56f47"},"execution_count":6,"outputs":[{"output_type":"stream","name":"stdout","text":["=== BẮT ĐẦU TRAINING (SMART SCHEDULER + EARLY STOPPING) ===\n","Epoch 1/100 | LR: 0.00300 | Train: 0.1027 | Val: 0.1064  [Save Best] 0.1064\n","Epoch 2/100 | LR: 0.00300 | Train: 0.0848 | Val: 0.1025  [Save Best] 0.1025\n","Epoch 3/100 | LR: 0.00300 | Train: 0.0826 | Val: 0.1022  [Save Best] 0.1022\n","Epoch 4/100 | LR: 0.00300 | Train: 0.0800 | Val: 0.1036  [Wait] 1/15\n","Epoch 5/100 | LR: 0.00300 | Train: 0.0796 | Val: 0.1018  [Save Best] 0.1018\n","Epoch 6/100 | LR: 0.00300 | Train: 0.0793 | Val: 0.1045  [Wait] 1/15\n","Epoch 7/100 | LR: 0.00300 | Train: 0.0792 | Val: 0.1032  [Wait] 2/15\n","Epoch 8/100 | LR: 0.00300 | Train: 0.0785 | Val: 0.1019  [Wait] 3/15\n","Epoch 9/100 | LR: 0.00300 | Train: 0.0776 | Val: 0.1029  [Wait] 4/15\n","Epoch 10/100 | LR: 0.00300 | Train: 0.0777 | Val: 0.1025  [Wait] 5/15\n","Epoch 11/100 | LR: 0.00150 | Train: 0.0777 | Val: 0.1026  [Wait] 6/15\n","Epoch 12/100 | LR: 0.00150 | Train: 0.0737 | Val: 0.1011  [Save Best] 0.1011\n","Epoch 13/100 | LR: 0.00150 | Train: 0.0734 | Val: 0.1011  [Wait] 1/15\n","Epoch 14/100 | LR: 0.00150 | Train: 0.0739 | Val: 0.1014  [Wait] 2/15\n","Epoch 15/100 | LR: 0.00150 | Train: 0.0740 | Val: 0.0998  [Save Best] 0.0998\n","Epoch 16/100 | LR: 0.00150 | Train: 0.0733 | Val: 0.0999  [Wait] 1/15\n","Epoch 17/100 | LR: 0.00150 | Train: 0