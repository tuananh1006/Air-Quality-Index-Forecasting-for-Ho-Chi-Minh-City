.eval()\n","print(\"--> Đã load Model tốt nhất.\")\n","\n","test_predictions, test_targets = [], []\n","with torch.no_grad():\n","    for batch_x, batch_y in test_loader:\n","        test_predictions.append(model(batch_x).cpu().numpy())\n","        test_targets.append(batch_y.cpu().numpy())\n","\n","# --- 2. Inverse Transform ---\n","# Đảm bảo scaler đã được fit trước đó\n","scaler_mean, scaler_scale = scaler.mean_[0], scaler.scale_[0]\n","y_pred_real = np.concatenate(test_predictions, axis=0) * scaler_scale + scaler_mean\n","y_true_real = np.concatenate(test_targets, axis=0) * scaler_scale + scaler_mean\n","\n","# Làm phẳng mảng để tính toán metrics\n","y_true_flat = y_true_real.flatten()\n","y_pred_flat = y_pred_real.flatten()\n","\n","# --- 3. Calculate Metrics ---\n","\n","# Các chỉ số cơ bản\n","mae = mean_absolute_error(y_true_flat, y_pred_flat)\n","mse = mean_squared_error(y_true_flat, y_pred_flat)\n","rmse = np.sqrt(mse)\n","mape = mean_absolute_percentage_error(y_true_flat, y_pred_flat)\n","r2 = r2_score(y_true_flat, y_pred_flat)\n","\n","# Xử lý riêng cho MSLE và RMSLE (tránh lỗi log số âm)\n","# Clip giá trị dự đoán và thực tế về tối thiểu là 0\n","y_true_safe = np.clip(y_true_flat, 0, None)\n","y_pred_safe = np.clip(y_pred_flat, 0, None)\n","\n","try:\n","    msle = mean_squared_log_error(y_true_safe, y_pred_safe)\n","    rmsle = np.sqrt(msle)\n","except Exception as e:\n","    print(f\"Lỗi tính MSLE/RMSLE: {e}\")\n","    msle, rmsle = 0.0, 0.0\n","\n","# --- 4. Print Results ---\n","print(\"\\n\" + \"=\"*45)\n","print(\"   KẾT QUẢ ĐÁNH GIÁ TOÀN DIỆN (FULL BENCHMARK)\")\n","print(\"=\"*45)\n","print(f\"MAE   (Sai số tuyệt đối trung bình):  {mae:.4f}\")\n","print(f\"MSE   (Bình phương sai số trung bình):{mse:.4f}\")\n","print(f\"RMSE  (Căn bậc 2 của MSE):            {rmse:.4f}\")\n","print(\"-\" * 45)\n","print(f\"MSLE  (Log sai số bình phương):       {msle:.4f}\")\n","print(f\"RMSLE (Căn bậc 2 của MSLE):           {rmsle:.4f}\")\n","print(\"-\" * 45)\n","print(f\"MAPE  (Sai số phần trăm):             {mape*100:.2f} %\")\n","print(f\"R²    (Hệ số xác định):               {r2:.4f}\")\n","print(\"=\"*45)\n","\n","# --- 5. Vẽ biểu đồ mẫu ---\n","plt.figure(figsize=(15, 6))\n","# Lấy 200 điểm đầu tiên của sensor đầu tiên (index 0) để vẽ\n","plt.plot(y_true_real[:200, 0, 0], label='Thực tế', color='black', alpha=0.7)\n","plt.plot(y_pred_real[:200, 0, 0], label='Dự báo STGCN', color='red', linewidth=1.5)\n","plt.title(\"So sánh Dự báo vs Thực tế (200 giờ đầu - Trạm 0)\")\n","plt.xlabel(\"Thời gian\")\n","plt.ylabel(\"Giá trị\")\n","plt.legend()\n","plt.grid(True, alpha=0.3) # Thêm lưới cho dễ nhìn\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":633},"id":"g3yOEVm35myA","executionInfo":{"status":"ok","timestamp":1764326323632,"user_tz":-420,"elapsed":723,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}},"outputId":"b364cae9-ad15-4bc9-e59f-6d74a770b1ec"},"execution_count":7,"outputs":[{"output_type":"stream","name":"stdout","text":["--> Đã load Model tốt nhất.\n","\n","=============================================\n","   KẾT QUẢ ĐÁNH GIÁ TOÀN DIỆN (FULL BENCHMARK)\n","=============================================\n","MAE   (Sai số tuyệt đối trung bình):  10.7040\n","MSE   (Bình phương sai số trung bình):427.8774\n","RMSE  (Căn bậc 2 của MSE):            20.6852\n","---------------------------------------------\n","MSLE  (Log sai số bình phương):       0.1545\n","RMSLE (Căn bậc 2 của MSLE):           0.3930\n","---------------------------------------------\n","MAPE  (Sai số phần trăm):             29.09 %\n","R²    (Hệ số xác định):               0.5344\n","=============================================\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1500x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["# New"],"metadata":{"id":"EXH6NDGGDlqH"}},{"cell_type":"code","source":["# ==========================================\n","# ĐỊNH NGHĨA MODEL STGCN ADAPTIVE (NÂNG CẤP)\n","# ==========================================\n","\n","class AdaptiveGraphConv(nn.Module):\n","    def __init__(self, c_in, c_out, num_nodes, dropout_rate=0.3):\n","        super(AdaptiveGraphConv, self).__init__()\n","        self.c_in = c_in\n","        self.c_out = c_out\n","        self.num_nodes = num_nodes\n","\n","        self.theta = nn.Parameter(torch.FloatTensor(c_in, c_out))\n","        self.bias = nn.Parameter(torch.FloatTensor(c_out))\n","        self.dropout = nn.Dropout(dropout_rate)\n","        self.reset_parameters()\n","\n","    def reset_parameters(self):\n","        nn.init.xavier_uniform_(self.theta)\n","        nn.init.zeros_(self.bias)\n","\n","    def forward(self, x, adj):\n","        # x: (Batch, c_in, T, Nodes) -> Permute to (Batch, T, Nodes, c_in)\n","        batch_size, c_in, T, num_nodes = x.shape\n","        x_tmp = x.permute(0, 2, 3, 1)\n","\n","        # 1. Biến đổi đặc trưng: (Batch, T, Nodes, c_out)\n","        x_mul = torch.matmul(x_tmp, self.theta)\n","\n","        # 2. Graph Convolution: AXW\n","        # adj: (Nodes, Nodes)\n","        # x_mul: (Batch, T, Nodes, c_out)\n","        # Kết quả: (Batch, T, Nodes, c_out)\n","        x_gconv = torch.einsum('nm,btnc->btmc', adj, x_mul)\n","\n","        x_gconv = x_gconv + self.bias\n","        return self.dropout(x_gconv.permute(0, 3, 1, 2))\n","\n","class STGCN_Block_Adaptive(nn.Module):\n","    def __init__(self, c_in, c_out, num_nodes, kt=3):\n","        super(STGCN_Block_Adaptive, self).__init__()\n","        self.conv_t1 = nn.Conv2d(c_in, c_out * 2, kernel_size=(1, kt), padding=(0, 1))\n","\n","        # Dùng Adaptive Graph Conv\n","        self.conv_s = AdaptiveGraphConv(c_out, c_out, num_nodes)\n","\n","        self.conv_t2 = nn.Conv2d(c_out, c_out * 2, kernel_size=(1, kt), padding=(0, 1))\n","        self.residual = nn.Conv2d(c_in, c_out, kernel_size=1) if c_in != c_out else None\n","        self.bn = nn.BatchNorm2d(c_out) # Thay LayerNorm bằng BatchNorm2d thường ổn định hơn cho Conv\n","\n","    def forward(self, x, adj):\n","        res = x\n","        if self.residual: res = self.residual(x)\n","\n","        # Temporal Conv 1\n","        x = self.conv_t1(x)\n","        p, q = x[:, :x.shape[1]//2, :, :], x[:, x.shape[1]//2:, :, :]\n","        x = p * torch.sigmoid(q) # Gated TCN\n","\n","        # Spatial Graph Conv (Adaptive)\n","        x = self.conv_s(x, adj)\n","\n","        # Temporal Conv 2\n","        x = self.conv_t2(x)\n","        p, q = x[:, :x.shape[1]//2, :, :], x[:, x.shape[1]//2:, :, :]\n","        x = p * torch.sigmoid(q)\n","\n","        return self.bn(x + res)\n","\n","class STGCN_Model_Adaptive(nn.Module):\n","    def __init__(self, num_nodes, num_features, static_adj, hidden_dim=64, node_dim=40):\n","        super(STGCN_Model_Adaptive, self).__init__()\n","\n","        # --- KEY: NODE EMBEDDINGS (ADAPTIVE MATRIX) ---\n","        # Hai ma trận E1, E2 để học mối quan hệ nguồn-đích\n","        self.node_emb1 = nn.Parameter(torch.randn(num_nodes, node_dim).to(device))\n","        self.node_emb2 = nn.Parameter(torch.randn(num_nodes, node_dim).to(device))\n","\n","        self.static_adj = static_adj # Giữ lại ma trận khoảng cách cũ làm gốc\n","        self.alpha = 0.3 # Tỷ trọng của ma trận học được\n","\n","        self.block1 = STGCN_Block_Adaptive(num_features, hidden_dim, num_nodes)\n","        self.block2 = STGCN_Block_Adaptive(hidden_dim, hidden_dim, num_nodes)\n","        self.block3 = STGCN_Block_Adaptive(hidden_dim, hidden_dim, num_nodes) # Thêm 1 block nữa cho sâu\n","\n","        self.final_conv = nn.Conv2d(hidden_dim, hidden_dim, kernel_size=1)\n","        self.output_layer = nn.Linear(hidden_dim, 1)\n","\n","    def forward(self, x):\n","        # Tính toán Adaptive Adjacency Matrix\n","        # A_adaptive = Softmax(Relu(E1 * E2^T))\n","        adp = F.softmax(F.relu(torch.mm(self.node_emb1, self.node_emb2.transpose(0, 1))), dim=1)\n","\n","        # Kết hợp ma trận tĩnh và ma trận động\n","        # Model vừa nhìn thấy khoảng cách địa lý, vừa tự học thêm quan hệ ngầm\n","        adj = self.static_adj + self.alpha * adp\n","\n","        x = self.block1(x, adj)\n","        x = self.block2(x, adj)\n","        x = self.block3(x, adj)\n","\n","        x = self.final_conv(x)\n","        x = x[:, :, -1, :] # Lấy bước thời gian cuối\n","        x = x.permute(0, 2, 1)\n","        x = self.output_layer(x)\n","        return x.permute(0, 2, 1)"],"metadata":{"id":"GE0g2Fv8DoRs","executionInfo":{"status":"ok","timestamp":1764327024433,"user_tz":-420,"elapsed":50,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}}},"execution_count":11,"outputs":[]},{"cell_type":"code","source":["# Đoạn code khởi tạo model mới\n","# Lưu ý: node_dim=40 là kích thước vector đại diện cho mỗi trạm để model học\n","model = STGCN_Model_Adaptive(\n","    num_nodes=num_nodes,\n","    num_features=num_features,\n","    static_adj=gso,\n","    hidden_dim=128,  # Tăng lên 128 để cạnh tranh với GRU\n","    node_dim=40\n",").to(device)\n","\n","print(\"--> Đã khởi tạo Adaptive STGCN - Ready to beat GRU!\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"vHVzVx1IFeXL","executionInfo":{"status":"ok","timestamp":1764327034632,"user_tz":-420,"elapsed":24,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}},"outputId":"24f1da72-2e0d-4c06-a679-fe8f95b94233"},"execution_count":12,"outputs":[{"output_type":"stream","name":"stdout","text":["--> Đã khởi tạo Adaptive STGCN - Ready to beat GRU!\n"]}]},{"cell_type":"code","source":["optimizer = torch.optim.Adam(model.parameters(), lr=0.001, weight_decay=1e-4)"],"metadata":{"id":"Y0tYGiKFFg3l","executionInfo":{"status":"ok","timestamp":1764327066390,"user_tz":-420,"elapsed":48,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}}},"execution_count":13,"outputs":[]},{"cell_type":"code","source":["import time\n","\n","# --- CẤU HÌNH ---\n","criterion = nn.L1Loss() # Dùng L1 Loss (MAE) để tối ưu, giúp model ít nhạy cảm với outlier hơn MSE\n","# Scheduler: Tự động giảm Learning Rate nếu model bị kẹt (không giảm lỗi)\n","scheduler = torch.optim.lr_scheduler.ReduceLROnPlateau(optimizer, mode='min', factor=0.5, patience=5)\n","\n","epochs = 150 # Model Adaptive cần nhiều epoch hơn để học ma trận kề\n","best_val_loss = float('inf')\n","save_path = 'best_stgcn_adaptive.pth'\n","loss_history = {'train': [], 'val': []}\n","\n","early_stopping_patience = 20 # Kiên nhẫn 20 epoch\n","early_stopping_counter = 0\n","\n","print(f\"=== BẮT ĐẦU TRAIN ADAPTIVE STGCN (Mục tiêu: Đánh bại GRU) ===\")\n","start_time = time.time()\n","\n","for epoch in range(epochs):\n","    # --- TRAIN ---\n","    model.train()\n","    train_loss = 0\n","    for batch_x, batch_y in train_loader:\n","        optimizer.zero_grad()\n","        output = model(batch_x) # Adaptive matrix tự tính bên trong model\n","        loss = criterion(output, batch_y)\n","        loss.backward()\n","        # Gradient Clipping để tránh bùng nổ gradient\n","        torch.nn.utils.clip_grad_norm_(model.parameters(), max_norm=5.0)\n","        optimizer.step()\n","        train_loss += loss.item()\n","\n","    # --- VALIDATE ---\n","    model.eval()\n","    val_loss = 0\n","    with torch.no_grad():\n","        for batch_x, batch_y in val_loader:\n","            output = model(batch_x)\n","            val_loss += criterion(output, batch_y).item()\n","\n","    # Tính trung bình loss\n","    avg_train_loss = train_loss / len(train_loader)\n","    avg_val_loss = val_loss / len(val_loader)\n","    loss_history['train'].append(avg_train_loss)\n","    loss_history['val'].append(avg_val_loss)\n","\n","    # Cập nhật Learning Rate\n","    current_lr = optimizer.param_groups[0]['lr']\n","    scheduler.step(avg_val_loss)\n","\n","    # In thông tin\n","    print(f\"Epoch {epoch+1}/{epochs} | LR: {current_lr:.6f} | Train Loss: {avg_train_loss:.4f} | Val Loss: {avg_val_loss:.4f}\", end='')\n","\n","    # --- SAVE BEST & EARLY STOPPING ---\n","    if avg_val_loss < best_val_loss:\n","        best_val_loss = avg_val_loss\n","        torch.save(model.state_dict(), save_path)\n","        early_stopping_counter = 0\n","        print(f\"  [SAVE BEST] ⭐\")\n","    else:\n","        early_stopping_counter += 1\n","        print(f\"  [WAIT] {early_stopping_counter}/{early_stopping_patience}\")\n","\n","        if early_stopping_counter >= early_stopping_patience:\n","            print(f\"\\n--> Dừng sớm (Early Stopping) tại epoch {epoch+1}. Model không cải thiện nữa.\")\n","            break\n","\n","print(f\"\\n--> Hoàn tất training sau {(time.time() - start_time)/60:.2f} phút.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"K-FrsSQ_Fonh","executionInfo":{"status":"ok","timestamp":1764329605005,"user_tz":-420,"elapsed":2423053,"user":{"displayName":"tuananh hocai","userId":"09485655636874666044"}},"outputId":"30761d60-08ee-497b-8899-155f5cf69860"},"execution_count":15,"outputs":[{"output_type":"stream","name":"stdout","text":["=== BẮT ĐẦU TRAIN ADAPTIVE STGCN (Mục tiêu: Đánh bại GRU) ===\n","Epoch 1/150 | LR: 0.001000 | Train Loss: 0.1742 | Val Loss: 0.1378  [SAVE BEST] ⭐\n","Epoch 2/150 | LR: 0.001000 | Train Loss: 0.1549 | Val Loss: 0.1140  [SAVE BEST] ⭐\n","Epoch 3/150 | LR: 0.001000 | Train Loss: 0.1519 | Val Loss: 0.1206  [WAIT] 1/20\n","Epoch 4/150 | LR: 0.001000 | Train Loss: 0.1601 | Val Loss: 0.1164  [WAIT] 2/20\n","Epoch 5/150 | LR: 0.001000 | Train Loss: 0.1565 | Val Loss: 0.1069  [SAVE BEST] ⭐\n","Epoch 6/150 | LR: 0.001000 | Train Loss: 0.1555 | Val Loss: 0.1602  [WAIT] 1/20\n","Epoch 7/150 | LR: 0.001000 | Train Loss: 0.1533 | Val Loss: 0.1051  [SAVE BEST] ⭐\n","Epoch 8/150 | LR: 0.001000 | Train Loss: 0.1526 | Val Loss: 0.1077  [WAIT] 1/20\n","Epoch 9/150 | LR: 0.001000 | Train Loss: 0.1522 | Val Loss: 0.1201  [WAIT] 2/20\n","Epoch 10/150 | LR: 0.001000 | Train Loss: 0.1525 | Val Loss: 0.1131  [WAIT] 3/